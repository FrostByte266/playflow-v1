{
  "name": "svelte-easyroute-ssr-template-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build -c vite.config.ts --target esnext && vite build --ssr ./src/App.ssr.svelte -c vite.config.ssr.ts --target es2018",
    "start": "vite preview",
    "start:ssr": "nodemon -e \".ts,.svelte\" -i dist --watch src --exec \"pnpm build && ts-node ./server.ssr.ts\""
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.47",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.39",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.1.0"
  },
  "dependencies": {
    "svelte-easyroute": "^3.1.2"
  }
}
